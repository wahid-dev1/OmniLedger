{
  "name": "omniledger",
  "version": "1.0.1",
  "description": "Desktop Inventory & Accounting System",
  "main": "dist-electron/main/main.js",
  "author": "wahid-dev1",
  "homepage": "https://github.com/wahid-dev1/omniledger",
  "repository": {
    "type": "git",
    "url": "https://github.com/wahid-dev1/omniledger.git"
  },
  "bugs": {
    "url": "https://github.com/wahid-dev1/omniledger/issues"
  },
  "keywords": [
    "inventory",
    "accounting",
    "desktop",
    "electron",
    "react",
    "typescript",
    "sequelize"
  ],
  "contributors": [
    {
      "name": "Wahid",
      "email": "wahid@gmail.com",
      "url": "https://github.com/wahid-dev1"
    }
  ],
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "os": [
    "win32",
    "darwin",
    "linux"
  ],
  "cpu": [
    "x64",
    "arm64"
  ],
  "packageManager": "npm@8.0.0",
  "scripts": {
    "predev": "npm run build:electron",
    "dev": "concurrently \"npm run dev:vite\" \"npm run dev:electron\"",
    "dev:vite": "vite",
    "dev:electron": "wait-on http://localhost:5173 -t 30000 && electron .",
    "build": "npm run build:vite && npm run build:electron",
    "build:vite": "vite build",
    "build:electron": "tsc -p tsconfig.electron.json",
    "build:win": "npm run build && electron-builder --win",
    "build:win:dir": "npm run build && electron-builder --win --dir",
    "build:mac": "npm run build && electron-builder --mac",
    "build:mac:dmg": "npm run build && electron-builder --mac dmg",
    "build:all": "npm run build && electron-builder --win --mac",
    "build:release": "npm run build && electron-builder --win --mac",
    "postinstall": "electron-rebuild -f -w sqlite3",
    "rebuild:electron": "electron-rebuild -f -w sqlite3",
    "preview": "vite preview",
    "seed": "ts-node --project tsconfig.seed.json scripts/seed.ts",
    "seed:database": "ts-node --project tsconfig.seed.json scripts/seed-database.ts",
    "lint": "eslint . --ext .ts,.tsx",
    "format": "prettier --write \"src/**/*.{ts,tsx,json,css}\""
  },
  "dependencies": {
    "sequelize": "^6.37.3",
    "better-sqlite3": "^11.2.0",
    "sqlite3": "^5.1.7",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "mysql2": "^3.9.4",
    "tedious": "^18.0.0",
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-alert-dialog": "^1.0.5",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-checkbox": "^1.0.4",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-separator": "^1.0.3",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.0.3",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-toast": "^1.1.5",
    "@types/jspdf": "^1.3.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "electron-updater": "^6.1.7",
    "exceljs": "^4.4.0",
    "jspdf": "^3.0.4",
    "jspdf-autotable": "^5.0.2",
    "lucide-react": "^0.303.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.21.1",
    "tailwind-merge": "^2.2.0",
    "tailwindcss-animate": "^1.0.7",
    "zustand": "^4.4.7",
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "body-parser": "^1.20.2",
    "jsonwebtoken": "^9.0.2",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.6",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@types/better-sqlite3": "^7.6.11",
    "@types/pg": "^8.11.5",
    "@types/express": "^4.17.21",
    "@types/cors": "^2.8.17",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "electron": "^28.1.0",
    "electron-builder": "^26.0.12",
    "electron-rebuild": "^3.2.9",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "fs-extra": "^11.3.2",
    "postcss": "^8.4.32",
    "prettier": "^3.1.1",
    "tailwindcss": "^3.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3",
    "vite": "^5.0.8",
    "vite-plugin-electron": "^0.28.1",
    "vite-plugin-electron-renderer": "^0.14.5",
    "wait-on": "^7.2.0"
  },
  "build": {
    "appId": "com.omniledger.app",
    "productName": "OmniLedger",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**/*",
      "dist-electron/**/*",
      "node_modules/**/*",
      "!node_modules/**/*.{md,ts,map}",
      "!node_modules/.cache",
      "!node_modules/**/{test,__tests__,tests,powered-test,example,examples}",
      "!node_modules/**/*.d.ts",
      "package.json"
    ],
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        },
        {
          "target": "portable",
          "arch": [
            "x64"
          ]
        }
      ],
      "icon": "build/icon.ico",
      "artifactName": "${productName}-${version}-${arch}.${ext}",
      "publisherName": "OmniLedger"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "OmniLedger"
    },
    "mac": {
      "target": [
        {
          "target": "dmg",
          "arch": [
            "x64",
            "arm64"
          ]
        },
        {
          "target": "zip",
          "arch": [
            "x64",
            "arm64"
          ]
        }
      ],
      "category": "public.app-category.finance",
      "icon": "build/icon.icns",
      "artifactName": "${productName}-${version}-${arch}.${ext}",
      "identity": null,
      "hardenedRuntime": true,
      "gatekeeperAssess": false,
      "entitlements": null,
      "entitlementsInherit": null
    },
    "dmg": {
      "title": "${productName} ${version}",
      "contents": [
        {
          "x": 410,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        },
        {
          "x": 130,
          "y": 150,
          "type": "file"
        }
      ],
      "window": {
        "width": 540,
        "height": 380
      }
    }
  }
}
